		"svelte-jester": "^2.0.1",
		"svelte-preprocess": "^4.9.4",
		"ts-jest": "^27.0.0",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"@interactjs/actions": "^1.10.11",
		"@interactjs/interact": "^1.10.11",
		"bootstrap": "^5.1.3"
	}
